    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "node-fetch": "^2.1.2",
    "patch-package": "^5.1.1",
    "react-hot-loader": "^4.1.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
