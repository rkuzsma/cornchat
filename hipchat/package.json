    "start:dev": "webpack-dev-server",
    "generate-dev-certs": "openssl req -x509 -newkey rsa:4096 -keyout dev-certs/key.pem -out dev-certs/cert.pem -days 365 -nodes -subj \"/CN=cornchat.development.fake/emailAddress=cornchat@fakedomain.com/O=Cornchat/OU=Development/C=US/ST=NH/L=Nowhere\"",
    "trust-dev-certs": "security add-trusted-cert -e hostnameMismatch -r trustRoot -k ~/Library/Keychains/login.keychain dev-certs/cert.pem"
  },
